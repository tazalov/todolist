{
  "name": "todolist",
  "version": "1.0.0",
  "homepage": "https://tazalov.github.io/todolist",
  "private": true,
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.14.3",
    "@mui/material": "^5.14.5",
    "@mui/x-date-pickers": "^6.12.0",
    "@reduxjs/toolkit": "^1.9.7",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^13.0.0",
    "@testing-library/user-event": "^13.2.1",
    "@types/jest": "^27.0.1",
    "@types/node": "^16.7.13",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "axios": "^1.5.1",
    "formik": "^2.4.5",
    "gh-pages": "^6.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.1.2",
    "react-router-dom": "^6.17.0",
    "react-scripts": "5.0.1",
    "redux": "^4.2.1",
    "redux-thunk": "^2.4.2",
    "styled-components": "^6.0.7",
    "typescript": "^4.4.2",
    "uuid": "^9.0.0",
    "web-vitals": "^3.5.0",
    "yup": "^1.3.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --transformIgnorePatterns \"node_modules/(?!axios)/\"",
    "lint:es": "eslint --ext .js,.jsx,.ts,.tsx src",
    "lint:es:fix": "npm run lint:es -- --fix",
    "eject": "react-scripts eject",
    "storybook": "storybook dev -p 9009",
    "build-storybook": "storybook build",
    "jest:integ": "jest -c integration/config/jest.config.js",
    "jest:integ:ok": "yarn run jest:integ --updateSnapshot",
    "test:integ": "start-server-and-test storybook http-get://localhost:9009 jest:integ",
    "sb:static:serve": "npx http-server storybook-static",
    "test:integ:sb:static": "start-server-and-test sb:static:serve 8080 jest:integ",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "prepare": "husky install"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@storybook/addon-essentials": "7.4.6",
    "@storybook/addon-interactions": "7.4.6",
    "@storybook/addon-links": "7.4.6",
    "@storybook/addon-onboarding": "1.0.8",
    "@storybook/addon-themes": "^7.5.1",
    "@storybook/blocks": "7.4.6",
    "@storybook/preset-create-react-app": "7.4.6",
    "@storybook/react": "7.4.6",
    "@storybook/react-webpack5": "7.4.6",
    "@storybook/testing-library": "0.2.2",
    "@types/react-redux": "^7.1.26",
    "@types/styled-components": "^5.1.26",
    "@types/uuid": "^9.0.2",
    "babel-plugin-named-exports-order": "0.0.2",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-storybook": "^0.6.14",
    "http-server": "^14.1.1",
    "husky": "^8.0.0",
    "i18next": "^23.7.6",
    "i18next-browser-languagedetector": "^7.2.0",
    "i18next-http-backend": "^2.4.1",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-image-snapshot": "^6.2.0",
    "jest-puppeteer": "^9.0.1",
    "prettier": "^3.0.3",
    "prop-types": "15.8.1",
    "puppeteer": "^21.3.6",
    "react-i18next": "^13.5.0",
    "start-server-and-test": "^2.0.1",
    "storybook": "7.4.6",
    "webpack": "5.88.2"
  },
  "jest": {
    "moduleNameMapper": {
      "axios": "axios/dist/node/axios.cjs"
    }
  }
}
